{
  "jenkins_base": "https://ci.athion.net/",
  "targets": [
    {
      "identifier": "new",
      "types": [
        {
          "job_name": "PlotSquared-Releases",
          "identifier": "release",
          "description": "Release builds for PlotSquared 1.13+. Requires WorldEdit 7 Beta-5!",
          "versions": [
            {
              "identifier": "bukkit",
              "artifact_pattern": "^PlotSquared-Bukkit-(?<version>[0-9.-]+).jar$"
            }
          ]
        },
        {
          "job_name": "PlotSquared-Breaking",
          "identifier": "dev",
          "description": "Development builds for PlotSquared 1.13+. Requires WorldEdit 7 Beta-5!",
          "versions": [
            {
              "identifier": "bukkit",
              "artifact_pattern": "^PlotSquared-Bukkit-(?<version>[0-9.]+).jar$"
            }
          ]
        }
      ]
    },
    {
      "identifier": "legacy",
      "types": [
        {
          "job_name": "PlotSquared-Legacy",
          "identifier": "dev",
          "description": "Development builds for PlotSquared <1.12",
          "versions": [
            {
              "identifier": "bukkit",
              "artifact_pattern": "^PlotSquared-Bukkit-(?<version>[A-Za-z0-9.-]+).jar$"
            },
            {
              "identifier": "nukkit",
              "artifact_pattern": "^PlotSquared-Nukkit-(?<version>[A-Za-z0-9.-]+).jar$"
            },
            {
              "identifier": "sponge",
              "artifact_pattern": "^PlotSquared-Sponge-(?<version>[A-Za-z0-9.-]+).jar$"
            }
          ]
        }
      ]
    }
  ]
}
